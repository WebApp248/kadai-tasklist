<% if logged_in? %>
  <%= link_to 'Logout', logout_path , method: :delete %></li>

  <h1>タスク一覧</h1>

  <ul>
    <% @tasks.each do |task| %>
      <li><%= link_to task.id, task %> : <%= task.status %> &gt; <%= task.content %></li>
    <% end %>
  </ul>

  <%= link_to '新規タスクの登録', new_task_path %>

<% else %>
  <%= link_to 'Sign up', signup_path %></li>
  <%= link_to 'Login', login_path %></li>
<% end %>
